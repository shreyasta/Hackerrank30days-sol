import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        /* Enter your code here. Read input from STDIN. Print output to STDOUT. Your class should be named Solution. */
        Scanner sc = new Scanner(System.in);  // for reading input
        int T =  sc.nextInt();                // for reading the test cases
        for (int i=0; i<T; i++ ){
            char[] inputString = sc.next().toCharArray();
            
            //Print even chars
            for (int j= 0; j < inputString.length; j=j+2 ){
                System.out.print(inputString[j]);
            }  
            
            System.out.print(" ");// for space
            //Print odd chars
            for (int j= 1; j < inputString.length; j=j+2 ){
                System.out.print(inputString[j]);
            }
               System.out.println();
        }
            
        
        sc.close();
        
        
    }
}
